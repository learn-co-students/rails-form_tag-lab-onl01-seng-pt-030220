<h3>Student Form</h3>

<%= form_tag students_path do %>
    <label for="student first name">Student first name:</label><br>
    <%= text_field_tag :'student[first_name]' %><br>

    <label for="student last name">Student last name</label><br>
    <%= text_field_tag :'student[last_name]'%><br>

    <%= submit_tag 'Submit Student'%>
<% end %>